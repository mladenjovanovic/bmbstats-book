@Book{jamesIntroductionStatisticalLearning2017,
  address = {{New York}},
  edition = {1st ed. 2013, Corr. 7th printing 2017 edition},
  title = {An {{Introduction}} to {{Statistical Learning}}: With {{Applications}} in {{R}}},
